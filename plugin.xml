<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.ui.popup.any?after=additions">
         <menu
               label="ProjectSummaryTool">
            <command
                  commandId="ProjectSummaryTool.generate"
                  id="generate"
                  label="generate"
                  style="push">
            </command>
         </menu>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="buaa.guanz.prosummary.ProjectSummaryClicker"
            commandId="ProjectSummaryTool.generate">
      </handler>
   </extension>

</plugin>
